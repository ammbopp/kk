<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controllers.StudentController">
   <children>
      <Label layoutX="101.0" layoutY="72.0" text="Student Name">
         <font>
            <Font name="Apple Symbols" size="24.0" />
         </font>
      </Label>
      <Label fx:id="nameLabel" layoutX="307.0" layoutY="72.0" text="Label" textFill="#60a7dd">
         <font>
            <Font name="Apple Symbols" size="24.0" />
         </font>
      </Label>
      <Label layoutX="101.0" layoutY="123.0" text="Student ID">
         <font>
            <Font name="Apple Symbols" size="24.0" />
         </font>
      </Label>
      <Label fx:id="idLabel" layoutX="307.0" layoutY="123.0" text="Label" textFill="#60a7dd">
         <font>
            <Font name="Apple Symbols" size="24.0" />
         </font>
      </Label>
      <Label layoutX="101.0" layoutY="175.0" text="Student Score">
         <font>
            <Font name="Apple Symbols" size="24.0" />
         </font>
      </Label>
      <Label fx:id="scoreLabel" layoutX="307.0" layoutY="175.0" text="Label" textFill="#60a7dd">
         <font>
            <Font name="Apple Symbols" size="24.0" />
         </font>
      </Label>
      <Button fx:id="backHello" layoutX="329.0" layoutY="222.0" onAction="#onHandleBackButton" text="Back">
         <mnemonicParsing>false</mnemonicParsing>
      </Button>
   </children>
</AnchorPane>
